    "repo": "https://github.com/td-mrs/minfill_mrs.git",
    "build_steps": [
      "make"
    ],
    "run_command": "./minfill_mrs < {input}",
    "run_mode": "stdin_stdout_signal",
    "input_format": "pace_gr",
    "output_format": "pace_td",
    "authors": "Philippe Jégou, Hanan Kanso, Cyril Terrioux",
    "pace_year": 2017,
    "description": "Min-fill heuristic with restarts"
  },
